{
  "name": "cthulhu/ios-receipt-parser",
  "description": "Utility to parse Apple-issued PKCS#7 container with receipts",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Denis Yuzhanin",
      "email": "cthulhu.den@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Cthulhu\\IosReceiptParser\\": "src/"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/N4M3Z/phpseclib"
    }
  ],
  "require": {
    "php": ">= 7.3",
    "phpseclib/phpseclib": "dev-fix/base64-octet-string"
  },
  "suggest": {
    "symfony/process": "To run OpenSSL in process to actually verify PKCS#7 container signatures"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3",
    "symfony/process": "^5.1",
    "dms/phpunit-arraysubset-asserts": "^0.2.0"
  }
}
